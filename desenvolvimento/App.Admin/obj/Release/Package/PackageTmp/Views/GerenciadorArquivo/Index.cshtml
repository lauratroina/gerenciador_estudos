@{
    ViewBag.Title = "Gerenciador de Arquivos";
}


<div class="page-header">
    <div class="row">
        <h1 class="col-xs-12 col-sm-4 text-center text-left-sm"><i class="fa fa-folder-open page-header-icon"></i>&nbsp;&nbsp;@ViewBag.Title</h1>
        <div class="col-xs-12 col-sm-8"></div>
    </div>
</div>


<!-- / Javascript -->
<iframe src="@Url.Content("~/Filemanager/")" style="width: 100%;" id="ifram-filemanager"></iframe>

<script>
    init.push(function () {
        $("#ifram-filemanager").height($(window).height() - 155);
        $(window).on("resize", function () {
            $("#ifram-filemanager").height($(window).height() - 155);
        });
    });
</script> 